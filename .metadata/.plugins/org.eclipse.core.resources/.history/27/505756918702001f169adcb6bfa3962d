package SwitCase;

public class Case {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String time = "braga";
		switch (time) {
		case "bragantino":
			
			System.out.println("Time da primeira divisão do São Paulo");
			break;
		case "palmeiras":
		System.out.println("Time campeão paulista");
		
		break;

		default:
			System.out.println("Todas as opções são inválidas");
			break;
		}

	}

}
